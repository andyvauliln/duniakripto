{
  "private": true,
  "name": "duniokripto",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "preact build --port 3000  --prerenderUrls ./prerender-urls.js",
    "serve": "sirv build --port 8080 --cors --single",
    "dev": "preact watch --prerenderUrls ./prerender-urls.js",
    "lint": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint-config-synacor",
    "rules": {
      "react/jsx-boolean-value": "never",
      "react/jsx-no-bind": [
        {
          "allowArrowFunctions": true
        }
      ]
    }
  },
  "eslintIgnore": [
    "build/*"
  ],
  "devDependencies": {
    "@quickbaseoss/babel-plugin-styled-components-css-namespace": "^1.0.1",
    "babel-plugin-styled-components": "^1.13.2",
    "eslint": "^4.9.0",
    "eslint-config-synacor": "^2.0.2",
    "esm": "^3.2.25",
    "glob": "^7.1.4",
    "jest": "^21.2.1",
    "jest-preset-preact": "^1.0.0",
    "markdown": "^0.5.0",
    "per-env": "^1.0.2",
    "preact-cli": "^3.0.0",
    "preact-render-spy": "^1.2.1",
    "sirv-cli": "1.0.3",
    "webpack-bundle-analyzer": "^3.4.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.15",
    "@preact/prerender-data-provider": "^3.0.0",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "markdown-to-jsx": "^6.10.3",
    "preact": "^10.0.0",
    "preact-cli-plugin-netlify": "^1.5.0",
    "preact-render-to-string": "^5.0.6",
    "preact-router": "^3.0.1",
    "react-typing-animation": "^1.6.2",
    "react-typist": "^2.0.5",
    "reactn": "^2.2.7",
    "styled-components": "^5.3.1",
    "styled-system": "^5.1.5",
    "timeago-react": "^3.0.4"
  },
  "jest": {
    "preset": "jest-preset-preact"
  },
  "prettier": {
    "printWidth": 160,
    "tabWidth": 1,
    "useTabs": true,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "none",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "avoid",
    "prettier.spaceBeforeFunctionParen": false
  }
}
